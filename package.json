{
  "name": "yoo-ui-pro",
  "version": "0.0.1",
  "main": "lib/yoo-ui-pro.cjs.js",
  "module": "lib/yoo-ui-pro.esm.mjs",
  "exports": {
    ".": {
      "require": "./lib/yoo-ui-pro.cjs.js",
      "import": "./lib/yoo-ui-pro.esm.mjs"
    },
    "./*": "./*"
  },
  "style": "lib/yoo-ui-pro.css",
  "files": [
    "lib",
    "README",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "rollup:lib": "rimraf ./lib && rollup -c rollup.config.js",
    "lint": "eslint . --ext .js,.ts,.tsx,.vue --ignore-path .eslintignore",
    "prepare": "husky install",
    "lint:style": "stylelint \"src/**/*.(less|css)\" --customSyntax postcss-less",
    "prepublish": "npm run rollup:lib"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Yoo-96/yoo-ui-pro.git"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.0.0",
    "element-plus": "^2.2.1",
    "screenfull": "^6.0.1",
    "unplugin-auto-import": "^0.6.1",
    "unplugin-vue-components": "^0.17.20",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^11.2.6",
    "path": "^0.12.7",
    "postcss-html": "^1.2.0",
    "postcss-less": "^5.0.0",
    "prettier": "^2.4.1",
    "rollup": "^2.74.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "stylelint": "^14.0.1",
    "stylelint-config-recommended-vue": "^1.0.0",
    "stylelint-config-standard": "^23.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.6.4",
    "vite-plugin-checker": "^0.4.6",
    "vue": "^3.2.36",
    "vue-tsc": "^0.34.16"
  }
}
